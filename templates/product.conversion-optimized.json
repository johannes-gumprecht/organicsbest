      "settings": {
        "custom_liquid": "<div class=\"product_page_divider\"><\/div>\n\n<div class=\"product_features\">\n\n    <div class=\"product_features_container\">\n\n        <div class=\"product_features_container_icon\">{{ product.metafields.uvp-1.svg-code }}<\/div>\n        <div class=\"product_features_container_content\">\n            <h4>{{ product.metafields.uvp-1.headline }}<\/h4>\n            <p>{{ product.metafields.uvp-1.copy }}<\/p>\n        <\/div>\n    <\/div>\n    <div class=\"product_features_container\">\n\n        <div class=\"product_features_container_icon\">{{ product.metafields.uvp-2.svg-code }}<\/div>\n        <div class=\"product_features_container_content\">\n            <h4>{{ product.metafields.uvp-2.headline }}<\/h4>\n            <p>{{ product.metafields.uvp-2.copy }}<\/p>\n        <\/div>\n    <\/div>\n    <div class=\"product_features_container\">\n\n        <div class=\"product_features_container_icon\">{{ product.metafields.uvp-3.svg-code }}<\/div>\n        <div class=\"product_features_container_content\">\n            <h4>{{ product.metafields.uvp-3.headline }}<\/h4>\n            <p>{{ product.metafields.uvp-3.copy }}<\/p>\n        <\/div>\n    <\/div>\n    <div class=\"product_features_container\">\n\n        <div class=\"product_features_container_icon\">{{ product.metafields.uvp-4.svg-code }}<\/div>\n        <div class=\"product_features_container_content\">\n            <h4>{{ product.metafields.uvp-4.headline }}<\/h4>\n            <p>{{ product.metafields.uvp-4.copy }}<\/p>\n        <\/div>\n    <\/div>\n<\/div>\n<div class=\"product_page_divider\"> <\/div>"
