    "164321041625547195": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "{% assign section1bulletsarray = product.metafields.copy.section-1-bullets | split: ',' %}\n{% assign section2bulletsarray = product.metafields.copy.section-2-bullets | split: ',' %}\n\n<div class=\"container customdescriptioncontainer\">\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-2\">\n        <h2>{{ product.metafields.copy.headline-1 }}<\/h2>\n        <h4>{{ product.metafields.copy.subheadline-1 }}<\/h4>\n        <p>{{ product.metafields.copy.text-1 }}<\/p>\n        <ul>{% for bullet in section1bulletsarray %}\n                <li>{{ bullet }}<\/li>\n            {% endfor %}\n        <\/ul>\n    <\/div>\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-1\">\n        {{ product.metafields.copy.video-1 }}\n    <\/div>\n<\/div>\n<div class=\"product_page_divider\"><\/div>\n\n<div class=\"container customdescriptioncontainer\">\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-1\">{{ product.metafields.copy.video-2 }}<\/div>\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-2\">\n        <h2>{{ product.metafields.copy.headline-2 }}<\/h2>\n        <h4>{{ product.metafields.copy.subheadline-1 }}<\/h4>\n        <p>{{ product.metafields.copy.text-2 }}<\/p>\n        <ul>{% for bullet in section2bulletsarray %}\n                <li>{{ bullet }}<\/li>\n            {% endfor %}\n        <\/ul>\n    <\/div>\n<\/div>"
      }
    },
    "16431066694cde1fec": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"product-accordion\"><dl class=\"accordion\">\n\n\n\n    <dt><a href=\"#\"><small class=\"right\">&#9660;<\/small>Preparation<\/a><\/dt>\n    <dd><div class=\"prod_tab_preparation_instr\">\n       <div class=\"prod_tab_preparation_instr_steps\">\n           {% assign preparation_steps =  product.metafields.tabs.preparation_steps | split: '_' %}\n           {% for preparation_step in preparation_steps%}\n               {% assign forloop.index = preparation_step_number %}\n               {% assign preparation_step_number = preparation_step_number | plus: 1 %}\n               <div class=\"prod_tab_preparation_instr_step\">\n                   <p>{{preparation_step_number}}.<\/p><span>{{preparation_step}}<\/span>\n               <\/div>\n           {% endfor %}\n       <\/div>\n       <div class=\"prod_tab_preparation_instr_notes\">\n           <p>{{product.metafields.tabs.preparation_notes}}<\/p>\n       <\/div>\n   <\/div><div class=\"prod_tab_item\" id=\"fourth_tab\" >\n                   <div class=\"prod_tab_table\">\n                      {{product.metafields.tabs.feedingtable}}\n                   <\/div>\n               <\/div> <\/dd>\n   \n    <dt><a href=\"#\"><small class=\"right\">&#9668;<\/small>Ingredients<\/a><\/dt>\n    <dd><p>{{ product.metafields.tabs.ingredients }}<\/p><div class=\"prod_tab_item\">\n        <div class=\"prod_tab_ingredients\">\n            {% assign ingredients = product.metafields.tabs.nutritionfacts | split: ';' %}\n            {% for ingredient in ingredients %}\n                {% assign ingredient_text = ingredient | split: '_' %}\n                <div class=\"ingredient_item\">\n                    <span>{{ ingredient_text[0] }}:<\/span><span>{{ ingredient_text[1] }}<\/span>\n                <\/div>\n            {% endfor %}\n        <\/div>\n        <p>{{ product.metafields.tabs.nutritionfactsnote }}<\/p>\n    <\/div><\/dd>\n   \n    <dt><a href=\"#\"><small class=\"right\">&#9668;<\/small>Nutrition facts<\/a><\/dt>\n    <dd><div class=\"prod_tab_item\">\n                       <div class=\"prod_tab_ingredients\">\n                           {% assign ingredients = product.metafields.tabs.nutritionfacts | split: ';' %}\n                           {% for ingredient in ingredients %}\n                               {% assign ingredient_text = ingredient | split: '_' %}\n                               <div class=\"ingredient_item\">\n                                   <span>{{ ingredient_text[0] }}:<\/span><span>{{ ingredient_text[1] }}<\/span>\n                               <\/div>\n                           {% endfor %}\n                       <\/div>\n                       <p>{{ product.metafields.tabs.nutritionfactsnote }}<\/p>\n                   <\/div> <\/dd>\n   \n    <dt><a href=\"#\"><small class=\"right\">&#9660;<\/small>Frequently Asked Questions<\/a><\/dt>\n    <dd>   <div class=\"prod_tab_faq\">\n        {% assign faqs = product.metafields.tabs.frequently_asked_questions | split: ';' %}\n        {% for faq in faqs %}\n            {% assign faq_text = faq | split: '_' %}\n            <div class=\"faq_item\">\n                <p><span class=\"faqquestion\">{{ faq_text[0] }}<\/span><br><span class=\"faqanswer\">{{ faq_text[1] }}<\/span><\/p>\n            <\/div>\n        {% endfor %}\n    <\/div><\/dd>\n   \n   <\/dl>\n   \n   <div class=\"bottom-guarantee center\"><h2 class=\"center\">Right Formula Guaranteeâ„¢<\/h2><h4 class=\"center\">We believe in our products and are going out of the way<br>to remove all possible worries for you.<\/h4><p>Testing out our premium products can open the question if your baby is going to accept the new formula.<\/p><p>Don't worry! We got you covered by our Happy Baby Guarantee!<\/p><p>If for any reason you experience an issue with the formula you bought from us,<br>we'll send you a gift card to try out another box or can of formula for free.<\/p><p>We will pay for it because we want you to find the best possible product for your little one.<\/p><\/div>\n   \n   \n   <\/div>"
      }
