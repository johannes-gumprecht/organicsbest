      "settings": {
        "custom_liquid": "{% assign section1bulletsarray = product.metafields.copy.section-1-bullets | split: ',' %}\n{% assign section2bulletsarray = product.metafields.copy.section-2-bullets | split: ',' %}\n\n<div class=\"container customdescriptioncontainer\">\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-2\">\n        <h2>{{ product.metafields.copy.headline-1 }}<\/h2>\n        <h4>{{ product.metafields.copy.subheadline-1 }}<\/h4>\n        <p>{{ product.metafields.copy.text-1 }}<\/p>\n        <ul>{% for bullet in section1bulletsarray %}\n                <li>{{ bullet }}<\/li>\n            {% endfor %}\n        <\/ul>\n    <\/div>\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-1\">\n        {{ product.metafields.copy.video-1 }}\n    <\/div>\n<\/div>\n<div class=\"product_page_divider\"><\/div>\n\n<div class=\"container customdescriptioncontainer\">\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-1\">{{ product.metafields.copy.video-2 }}<\/div>\n    <div class=\"one-half columns medium-down--one-whole customdescriptioncolumn flexorder-2\">\n        <h2>{{ product.metafields.copy.headline-2 }}<\/h2>\n        <h4>{{ product.metafields.copy.subheadline-1 }}<\/h4>\n        <p>{{ product.metafields.copy.text-2 }}<\/p>\n        <ul>{% for bullet in section2bulletsarray %}\n                <li>{{ bullet }}<\/li>\n            {% endfor %}\n        <\/ul>\n    <\/div>\n<\/div>"
      }
